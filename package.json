{
  "name": "bicycle-keeper",
  "version": "1.0.0",
  "description": "bicycle-keeper",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon --inspect server.js",
    "client": " npm start --prefix client/",
    "clientinstall": "npm install --perfix client",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "build": "webpack" ,
    "heroku-postbuild":"NPM_CONFIG_PRODUCTION=false        cd client/ && npm install --only=dev && NODE_ENV=development npm install --no-shrinkwrap && npm run build "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.0.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.7"
  }
}
